Host *
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/id_rsa

Host github.com
  Hostname ssh.github.com
  Port 443

Host bitbucket.org
  Hostname altssh.bitbucket.org
  Port 443

Host aiido-production
    IdentityFile ~/.ssh/aiido-production-52.77.96.68.pem

Host aiido-staging
    IdentityFile ~/.ssh/aiido-staging-13.228.82.252.pem
